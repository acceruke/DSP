
import numpy as np
import matplotlib.pyplot as plt
   
A = 1
f = 5
phase1 = 0
phase2 = (2/3)*np.pi
phase3 = (4/3)*np.pi
t = np.linspace(0, 1,10000)

y1 = A*np.sin(2*np.pi*f*t + phase1)
y2 = A*np.sin(2*np.pi*f*t + phase2)
y3 = A*np.sin(2*np.pi*f*t + phase3)

plt.subplot(3,1, 1)
plt.plot(t, y1)
plt.grid()
plt.tight_layout()
plt.xlabel('time(sec)')
plt.ylabel('Amplitude(Volts)')
plt.title('Sinusoidal wave')

plt.subplot(3,1, 2)
plt.plot(t, y2)
plt.grid()
plt.tight_layout()
plt.xlabel('time(sec)')
plt.ylabel('Amplitude(Volts)')
plt.title('Sinusoidal wave')

plt.subplot(3,1, 3)
plt.plot(t, y3)
plt.grid()
plt.tight_layout()
plt.xlabel('time(sec)')
plt.ylabel('Amplitude(Volts)')
plt.title('Sinusoidal wave')


